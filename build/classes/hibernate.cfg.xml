<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/histogene?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">1234</property>
    <property name="hibernate.hbm2ddl.auto">update</property>
    <mapping class="" file="" jar="" package="" resource="CRUD/Usuario/Usuario.hbm.xml"/>
    <mapping resource="CRUD/Resultados/resultados.hbm.xml"/>
    <mapping resource="CRUD/Exames/exames.hbm.xml"/>
    <mapping resource="CRUD/Pacientes/paciente.hbm.xml"/>
    <mapping resource="CRUD/Pagamentos/pagamentos.hbm.xml"/>
    <mapping resource="CRUD/SalaEquipamentos/Itens/item.hbm.xml"/>
    <mapping resource="CRUD/SalaEquipamentos/sala.hbm.xml"/>
    <mapping resource="CRUD/Pedidos/pedido.hbm.xml"/>
    <mapping resource="CRUD/Materiais/materiais.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
